{
  "name": "property-tracker",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@sentry/cli",
      "core-js",
      "esbuild",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "playwright test e2e/accessibility.spec.ts --project=chromium",
    "test:a11y:public": "playwright test e2e/accessibility.spec.ts --project=chromium --grep='Public Pages'",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ui-audit": "playwright test e2e/ui-audit/ --project=chromium",
    "test:ui-audit:public": "playwright test e2e/ui-audit/public/ --project=chromium",
    "test:ui-audit:dashboard": "playwright test e2e/ui-audit/dashboard/ --project=chromium",
    "test:ui-audit:reports": "playwright test e2e/ui-audit/reports/ --project=chromium",
    "test:ui-audit:settings": "playwright test e2e/ui-audit/settings/ --project=chromium",
    "test:core-loop": "playwright test --project=core-loop",
    "test:core-loop:headed": "playwright test --project=core-loop --headed",
    "test:core-loop:debug": "playwright test --project=core-loop --debug",
    "ui-audit:report": "npx tsx scripts/generate-audit-report.ts",
    "ui-audit:full": "npm run test:ui-audit && npm run ui-audit:report",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "seed:demo": "npx tsx src/scripts/seed.ts demo",
    "seed:dev": "npx tsx src/scripts/seed.ts dev",
    "seed:clean": "npx tsx src/scripts/seed.ts clean",
    "changelog:sync": "npx tsx scripts/sync-changelog.ts",
    "blog:sync": "npx tsx scripts/sync-blog.ts"
  },
